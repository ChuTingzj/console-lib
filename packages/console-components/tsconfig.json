{
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": true,
		"jsx": "react-jsx",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"declaration": true,
		"declarationDir": "./dist",
		"emitDeclarationOnly": false,
		"lib": ["ESNext", "DOM"],
		"skipLibCheck": true,
		"noEmit": true,
		"types": ["node", "vite/client"]
	},
	"include": [
		"lib/**/**/*.tsx",
		"lib/**/**/*.vue",
		"lib/**/*.vue",
		"lib/**/*.tsx",
		"lib/*.vue",
		"lib/*.ts",
		"lib/*.tsx",
		"src/*.ts",
		"src/*.d.ts"
	],
	"exclude": ["node_modules"]
}
