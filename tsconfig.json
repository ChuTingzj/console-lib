{
	"compilerOptions": {
		"target": "ES2016",
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"removeComments": false
	}
}
